<animation>
  <type>pick</type>
  <object-name>apoff</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
	<binding>
		<command>property-assign</command>
		<property>/it-autoflight/input/ap1</property>
		<value>0</value>
	</binding>
	<binding>
		<command>property-assign</command>
		<property>/it-autoflight/input/ap2</property>
		<value>0</value>
	</binding>
    <binding>
